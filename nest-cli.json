{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mymicroservice/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mymicroservice/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mymicroservice",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "mymicroservice": {
      "type": "application",
      "root": "apps/mymicroservice",
      "entryFile": "main",
      "sourceRoot": "apps/mymicroservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mymicroservice/tsconfig.app.json"
      }
    },
    "quetions": {
      "type": "application",
      "root": "apps/quetions",
      "entryFile": "main",
      "sourceRoot": "apps/quetions/src",
      "compilerOptions": {
        "tsConfigPath": "apps/quetions/tsconfig.app.json"
      }
    },
    "role": {
      "type": "application",
      "root": "apps/role",
      "entryFile": "main",
      "sourceRoot": "apps/role/src",
      "compilerOptions": {
        "tsConfigPath": "apps/role/tsconfig.app.json"
      }
    },
    "test": {
      "type": "application",
      "root": "apps/test",
      "entryFile": "main",
      "sourceRoot": "apps/test/src",
      "compilerOptions": {
        "tsConfigPath": "apps/test/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}
